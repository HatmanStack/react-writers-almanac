{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
      "requireConfigFile": false,
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "extends": ["eslint:recommended"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "react/prop-types": 0,
    "no-console": 1
  }
}